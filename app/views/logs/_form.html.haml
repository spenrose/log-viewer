= form_for(@log , :html => { :multipart => true}) do |f|
  -if @log.errors.any?
    #error_explanation
      %h2= pluralize(@log.errors.count, "error") 
      prohibited this log from being saved:

      %ul
      -@log.errors.full_messages.each do |msg|
        %li= msg

  .field
    = f.label :name
    %br
    = f.text_field :name

  .field
    = f.label :log
    %br
    = f.file_field :log

  .actions
    = f.submit

